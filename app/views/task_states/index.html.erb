<h1>Listing task_states</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @task_states.each do |task_state| %>
  <tr>
    <td><%= task_state.name %></td>
    <td><%= link_to 'Show', task_state %></td>
    <td><%= link_to 'Edit', edit_task_state_path(task_state) %></td>
    <td><%= link_to 'Destroy', task_state, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

